{
  "name": "compra-do-mes",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.11",
    "font-awesome": "^4.7.0",
    "fs-extra": "5.0.0",
    "html-webpack-plugin": "3.0.4",
    "inferno": "^5.0.4",
    "inferno-compat": "^5.0.5",
    "inferno-component": "^5.0.4",
    "inferno-create-class": "^5.0.4",
    "inferno-dev-utils": "^5.3.0",
    "inferno-redux": "^5.0.4",
    "inferno-router": "^5.0.4",
    "jest": "22.4.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "2.1.1",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "redux": "^3.7.2",
    "style-loader": "0.20.2",
    "sw-precache-webpack-plugin": "0.11.4",
    "tsconfig-paths-webpack-plugin": "^3.1.2",
    "tslint-config-prettier": "^1.12.0",
    "url-loader": "1.0.1",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack -p",
    "lint": "tslint src/*.ts{,x} src/**/*.ts{,x}",
    "start": "webpack-dev-server --inline",
    "predeploy": "npm run build",
    "deploy": "surge ./build -d https://compra-do-mes.surge.sh"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-inferno": "^4.0.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "clean-webpack-plugin": "^0.1.18",
    "html-webpack-plugin": "^2.30.1",
    "source-map-loader": "^0.2.3",
    "ts-loader": "^3.5.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "3.11.0",
    "webpack-dev-server": "2.11.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react$": "inferno-compat",
      "^react-dom$": "inferno-compat"
    },
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "inferno-app"
    ]
  },
  "eslintConfig": {
    "extends": "inferno-app"
  }
}
